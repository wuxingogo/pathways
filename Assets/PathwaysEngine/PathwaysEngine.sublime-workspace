aller; getMenu 		= true; break;\n				case \"OnJump\" 	: onJump 	+= caller; getJump 		= true; break;\n				case \"OnCrouch\" : onCrouch	+= caller; getCrouch	= true; break;\n				case \"OnSprint\" : onSprint	+= caller; getSprint	= true; break;\n				case \"OnLamp\"	: onLamp	+= caller; getLamp 		= true; break;\n			}\n		}\n\n		public static void AddAxisListener(inputAxis caller) {\n			switch (caller.Method.Name) {\n				case \"OnAxisX\"	: 	onAxisX 	+= caller; getAxisX 	= true; break;\n				case \"OnAxisY\" 	: 	onAxisY 	+= caller; getAxisY 	= true; break;\n				case \"OnMouseX\" : 	onMouseX 	+= caller; getMouseX 	= true; break;\n				case \"OnMouseY\" : 	onMouseY 	+= caller; getMouseY 	= true; break;\n				case \"OnRoll\" 	: 	onRoll 		+= caller; getRoll 		= true; break;\n			}\n		}\n	}\n}\n",
